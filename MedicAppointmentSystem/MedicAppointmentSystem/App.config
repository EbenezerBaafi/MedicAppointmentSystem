<?xml version="1.0" encoding="utf-8"?>
<configuration>
	<startup>
		<supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
	</startup>

	<connectionStrings>
		<!-- SQL Server with Integrated Security (Windows Authentication) -->
		<add name="MedicalDB"
             connectionString="Data Source=(localdb)\MSSQLLocalDB;Initial Catalog=MedicalDB;Integrated Security=True"
             providerName="System.Data.SqlClient" />

		<!-- Alternative: SQL Server Authentication (if needed) -->
		<!-- 
        <add name="MedicalDB" 
             connectionString="Data Source=YourServerName;Initial Catalog=MedicalDB;User ID=YourUsername;Password=YourPassword" 
             providerName="System.Data.SqlClient" /> 
        -->
	</connectionStrings>
</configuration>